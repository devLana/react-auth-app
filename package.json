{
  "name": "react-auth-app",
  "private": true,
  "version": "1.0.0",
  "description": "An app with basic front end authentication",
  "scripts": {
    "build": "webpack --config ./configs/webpack.prod.js",
    "dev": "webpack serve --open --config ./configs/webpack.dev.js",
    "server:build": "webpack --config ./configs/webpack.server.js --env production -w",
    "server:start": "nodemon ./server-build/js/server.bundle.js",
    "ssr": "yarn build && yarn server:build && yarn server:start"
  },
